<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="ruby">
  <template name="perr" value="puts &quot;--- ERROR &quot;+&quot;-&quot;*50, &quot;#{e.class}:#{e}&quot;, *e.backtrace.first(10)" description="puts exception info" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="false" />
      <option name="XML" value="false" />
      <option name="RUBY" value="true" />
      <option name="RHTML" value="false" />
      <option name="HAML" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="CSS" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
</templateSet>

